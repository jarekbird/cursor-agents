{
  "name": "cursor-agents",
  "version": "1.0.0",
  "description": "Node.js application for managing recurring/ongoing prompts using BullMQ",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "start": "tsx src/index.ts",
    "start:prod": "node dist/index.js",
    "start:mcp": "tsx src/mcp/index.ts",
    "start:mcp:prod": "node dist/mcp/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --passWithNoTests",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage --passWithNoTests",
    "test:unit": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern='tests/(queue|mcp|services)' --passWithNoTests",
    "test:http": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern='tests/(app|integration)\\.test' --passWithNoTests",
    "test:e2e-lite": "NODE_OPTIONS=--experimental-vm-modules jest --testPathPattern='tests/e2e' --passWithNoTests",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{js,ts}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts}\"",
    "inspect:db": "tsx scripts/inspect-db-schema.ts"
  },
  "keywords": [
    "bullmq",
    "queue",
    "recurring-prompts",
    "cursor-agents"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@bull-board/api": "^6.14.2",
    "@bull-board/express": "^6.14.2",
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^12.4.1",
    "bullmq": "^5.0.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
